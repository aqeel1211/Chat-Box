<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor" prefHeight="425.0" prefWidth="724.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.windows.vimo.screens.LogInController">
   <children>
      <ImageView fitHeight="94.0" fitWidth="125.0" layoutX="324.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/hipchat.png" />
         </image>
      </ImageView>
      <Label layoutX="326.0" layoutY="115.0" text="Vimo" textFill="#4e96da">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="371.0" layoutY="115.0" text="Chat">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="495.0" layoutY="20.0" text="New Member?">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Label>
      <Button fx:id="signUpBtn" layoutX="604.0" layoutY="10.0" mnemonicParsing="false" onAction="#signUpPressed" prefHeight="39.0" prefWidth="106.0" style="-fx-background-radius: 100;" stylesheets="@application.css" text="SIGN UP" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <TextField fx:id="username" layoutX="278.0" layoutY="198.0" prefHeight="26.0" prefWidth="187.0" promptText="Username">
