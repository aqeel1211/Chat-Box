      <ImageView fitHeight="30.0" fitWidth="40.0" layoutX="259.0" layoutY="198.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Desktop/user.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="30.0" fitWidth="40.0" layoutX="259.0" layoutY="288.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Desktop/padlock.png" />
         </image>
      </ImageView>
      <Button fx:id="logInBtn1" layoutX="279.0" layoutY="336.0" mnemonicParsing="false" onAction="#logInPressed" prefHeight="39.0" prefWidth="187.0" style="-fx-background-radius: 100;" stylesheets="@application.css" text="LOG IN" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <PasswordField fx:id="password" layoutX="278.0" layoutY="273.0" prefHeight="30.0" prefWidth="187.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
